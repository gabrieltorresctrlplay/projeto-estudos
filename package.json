{
  "name": "oiee",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:emulator": "set VITE_USE_EMULATORS=true&& set VITE_FIREBASE_API_KEY=demo-api-key&& set VITE_FIREBASE_PROJECT_ID=demo-projeto-estudos&& vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy --only hosting",
    "emulators": "firebase emulators:start --project demo-projeto-estudos --import=./emulator-data --export-on-exit=./emulator-data",
    "ci": "firebase emulators:start --project demo-projeto-estudos --import=./emulator-data --export-on-exit",
    "format": "prettier --write .",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "cli": "start \"\" \"C:\\Program Files\\PowerShell\\7\\pwsh.exe\" -WorkingDirectory ."
  },
